{
  "name": "bzga/bzga-beratungsstellensuche-essstoerungen",
  "type": "typo3-cms-extension",
  "description": "Beratungsstellensuche der BZgA fÃ¼r den Bereich Schwangerschaft",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/sabbelasichon/bzga_beratungsstellensuche.git"
    }
  ],
  "authors": [
    {
      "name": "Sebastian Schreiber",
      "email": "ssch@hauptweg-nebenwege.de",
      "role": "Developer"
    },
    {
      "name": "Sebastian Hebler",
      "email": "sh@hauptweg-nebenwege.de",
      "role": "Projectmanager"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bzga\\BzgaBeratungsstellensucheEssstoerungen\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bzga\\BzgaBeratungsstellensucheEssstoerungen\\Tests\\": "Tests/"
    }
  },
  "require": {
    "typo3/cms-core": "^8.7",
    "php": ">=7.0.0",
    "bzga/bzga-beratungsstellensuche": "8.7.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^1.0 || ^2.0 || ^3.0 || ^4.0"
  },
  "replace": {
    "bzga_beratungsstellensuche_essstoerungen": "self.version",
    "bzga/bzga-beratungsstellensuche-essstoerungen": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/bzga_beratungsstellensuche_essstoerungen ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/bzga_beratungsstellensuche_essstoerungen"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.7.x-dev"
    },
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}

